import java.util.Scanner;<div></div><br/>/**<div></div> *<div></div> * @author Admin<div></div> */<div></div>public class Restore{<div></div>    public static void main(String[] args) {<div></div><br/>        Scanner scanner=new Scanner(System.in);<div></div>        System.out.println("Path:");<div></div>        String path=scanner.nextLine();<div></div>        System.out.println("Username:");<div></div>        String username=scanner.nextLine();<div></div>        System.out.println("Password:");<div></div>        String password=scanner.nextLine();<div></div>        System.out.println("Database name:");<div></div>        String dbname=scanner.nextLine();<div></div><br/>        String[] restoreCmd = new String[]{"mysql ", "--user=" + username, "--password=" + password,dbname,"-e", "source " + path};<div></div>    try {<div></div>        Process runtimeProcess = Runtime.getRuntime().exec(restoreCmd);<div></div>        int processComplete = runtimeProcess.waitFor();<div></div>        if (processComplete == 0) {<div></div>            System.out.println("Restore succesful");<div></div>        } else {<div></div>            System.out.println("Restore Failed");<div></div>        }<div></div>    } catch (Exception ex) {<div></div>        ex.printStackTrace();<div></div>    }<div></div>    }<div></div>}<div></div>